<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="edu.yzk.qrcodestatistics">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".qrcode.GeneralCaptureActivity"/>
        <activity android:name=".issueStatistics.IssueActivity">
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:theme="@android:style/Theme.NoTitleBar">
        </activity>
        <activity android:name=".form.FormActivity">
        </activity>
        <activity android:name=".Login.LoginActivity">
        </activity>
        <activity android:name=".issueStatistics.EncodeActivity">
        </activity>
        <activity android:name=".inspect.InspectActivity">
        </activity>
        <activity android:name=".Login.SignUpActivity">
        </activity>
        <activity android:name=".myStatistics.MyStatisticsActivity">
        </activity>
        <activity android:name=".myStatistics.ShowFormsActivity">
        </activity>
    </application>

</manifest>